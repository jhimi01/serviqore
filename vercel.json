{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "25mb"
        }
      },
      {
        "src": "build/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "build/$1" }
    ]
  }
  